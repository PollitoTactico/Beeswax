<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Beeswax.Views.InicioShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Beeswax.Views"
    Title="Inicio">
    <TabBar>
        <Tab Title="Home" Icon="home.png">
            <ShellContent
                Title=""
                ContentTemplate="{DataTemplate local:InicioPage}"
                Route="InicioPage" />
        </Tab>
        <Tab Title="Compra" Icon="cart.png">
            <ShellContent
                Title=""
                ContentTemplate="{DataTemplate local:Pagina2}"
                Route="Pagina2" />
        </Tab>
    </TabBar>
</Shell>
